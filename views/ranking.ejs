<!DOCTYPE html>
<html>
    <head>
        <title>Ranking information : <%= start %> to <%= Number(start) + Number(limit) %></title>
        <link rel='stylesheet' href='/stylesheets/style.css' />
    </head>
    <body>
        <table class="hovertable">
            <tr>
                <th>name</th>
                <th>score</th>
                <th>rank</th>
            </tr>
            <% for(var i = 0; i < data.length / 2; i++) { %>
                <tr>
                    <td><%= data[i * 2] %></td>
                    <td><%= data[i * 2 + 1] %></td>
                    <td><%= (parseInt(start) + i) %></td>
                </tr>
            <% } %>
        </table>
    </body>
</html>